    "react-redux": "5.0.7",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-spring": "5.6.9",
    "redux": "4.0.0",
    "redux-thunk": "2.3.0",
    "reselect": "3.0.1",
    "styled-components": "3.4.5",
    "styled-icons": "2.3.2",
