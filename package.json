    "@storybook/addon-knobs": "4.0.0-alpha.18",
    "@storybook/addon-options": "4.0.0-alpha.18",
    "@storybook/addon-viewport": "4.0.0-alpha.18",
    "@storybook/react": "4.0.0-alpha.18",
    "@types/color": "3.0.0",
    "@types/copy-webpack-plugin": "4.4.2",
    "@types/del": "3.0.1",
    "@types/electron-debug": "1.1.0",
    "@types/electron-devtools-installer": "2.2.0",
    "@types/electron-store": "1.3.0",
    "@types/friendly-errors-webpack-plugin": "0.1.1",
    "@types/gulp": "4.0.5",
    "@types/gulp-changed": "0.0.32",
    "@types/hard-source-webpack-plugin": "0.9.0",
    "@types/html-webpack-plugin": "3.2.0",
    "@types/i18next-xhr-backend": "1.4.1",
    "@types/jest": "23.3.1",
    "@types/koa-static": "4.0.0",
    "@types/koa-webpack": "5.0.1",
    "@types/lodash.debounce": "4.0.4",
    "@types/node": "10.9.2",
    "@types/ramda": "0.25.36",
    "@types/react": "16.4.11",
    "@types/react-dom": "16.0.7",
    "@types/react-hot-loader": "4.1.0",
    "@types/react-i18next": "7.8.2",
    "@types/react-redux": "6.0.6",
    "@types/react-router-dom": "4.3.0",
    "@types/serve-static": "1.13.2",
    "@types/storybook__addon-knobs": "3.4.1",
    "@types/storybook__addon-options": "3.2.2",
    "@types/storybook__react": "3.0.9",
    "@types/styled-system": "3.0.3",
    "@types/tapable": "1.0.4",
    "@types/tar-fs": "1.16.1",
    "@types/webpack": "4.4.11",
    "@types/webpack-env": "1.13.6",
    "@types/xtend": "4.0.2",
    "@types/yauzl": "2.9.0",
    "babel-loader": "8.0.0-beta.6",
    "commander": "2.17.1",
    "copy-webpack-plugin": "4.5.2",
    "css-loader": "1.0.0",
    "del": "3.0.0",
    "electron": "3.0.0-beta.7",
    "electron-builder": "20.28.2",
    "electron-packager": "12.1.1",
    "fork-ts-checker-webpack-plugin": "0.4.9",
    "friendly-errors-webpack-plugin": "1.7.0",
    "gulp": "4.0.0",
    "gulp-changed": "3.2.0",
    "hard-source-webpack-plugin": "0.12.0",
    "html-webpack-plugin": "3.2.0",
    "imagemin": "6.0.0",
    "imagemin-pngquant": "6.0.0",
    "img-loader": "vanwagonet/img-loader",
    "jest": "23.5.0",
    "jest-styled-components": "6.1.1",
    "koa": "2.5.2",
    "koa-static": "5.0.0",
    "koa-webpack": "5.1.0",
    "less": "3.8.1",
    "less-loader": "4.1.0",
    "react-hot-loader": "4.3.5",
    "serve-static": "1.13.2",
    "spectron": "3.8.0",
    "style-loader": "0.22.1",
    "svg-sprite-loader": "3.9.2",
    "tree-kill": "1.2.0",
    "ts-import-plugin": "1.5.4",
    "ts-jest": "23.1.4",
    "ts-loader": "4.5.0",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "typescript": "3.0.1",
    "url-loader": "1.1.1",
    "webpack": "4.17.1",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-dev-middleware": "3.2.0",
    "xtend": "4.0.1"
  },
  "engines": {
    "node": ">=10.1.0",
    "npm": ">=6.0.1"
  },
  "jest": {
    "roots": [
      "app/src"
    ],
    "collectCoverageFrom": [
      "**/*.{ts,tsx}"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleDirectories": [
      "node_modules",
      "build/node_modules"
    ],
    "moduleNameMapper": {
      "^@assets/(.*)": "<rootDir>/app/src/renderer/__mocks__/file-mock.ts",
      "^@common/(.*)": "<rootDir>/app/src/common/$1",
      "^@app-utils/(.*)": "<rootDir>/app/src/main/utils/$1",
      "^@view-utils/(.*)": "<rootDir>/app/src/renderer/utils/$1",
      "^@view-logic/(.*)": "<rootDir>/app/src/renderer/logic/$1",
      "^@components/(.*)": "<rootDir>/app/src/renderer/components/$1"
    }
  },
  "build": {
    "appId": "com.electron.pinkcoin",
    "productName": "Pinkcoin Wallet UI",
    "directories": {
      "app": "build",
      "output": "dist"
    },
    "publish": {
      "provider": "github"
    }
  }
}
