{
  "private": true,
  "name": "pink-wallet",
  "version": "0.1.0",
  "description": "Pinkcoin wallet desktop UI",
  "author": "Astrych",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Astrych/pink-wallet"
  },
  "bugs": {
    "url": "https://github.com/Astrych/pink-wallet/issues"
  },
  "keywords": [
    "PINK",
    "Pinkcoin",
    "Charity",
    "Blockchain"
  ],
  "scripts": {
    "start": "gulp clean & gulp prepare & gulp start",
    "analyzer": "gulp clean & gulp prepare & gulp start -a",
    "clean": "gulp clean",
    "clean-all": "gulp clean-all",
    "builder": "gulp builder --env=production --platform=all",
    "linux-builder": "gulp builder --env=production --platform=linux",
    "windows-builder": "gulp builder --env=production --platform=windows",
    "mac-builder": "gulp builder --env=production --platform=mac",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "dependencies": {
    "antd": "3.7.0",
    "i18next": "11.3.6",
    "i18next-xhr-backend": "1.5.1",
    "immer": "1.3.1",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-i18next": "7.9.0",
    "react-redux": "5.0.7",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-spring": "5.4.2",
    "redux": "4.0.0",
    "redux-thunk": "2.3.0",
    "reselect": "3.0.1",
    "styled-components": "3.3.3",
    "styled-icons": "2.3.1"
  },
  "devDependencies": {
    "@types/del": "3.0.1",
    "@types/electron-debug": "1.1.0",
    "@types/electron-devtools-installer": "2.0.3",
    "@types/electron-store": "1.3.0",
    "@types/gulp": "4.0.5",
    "@types/gulp-changed": "0.0.32",
    "@types/i18next-xhr-backend": "1.4.1",
    "@types/jest": "23.3.0",
    "@types/koa-static": "4.0.0",
    "@types/koa-webpack": "5.0.1",
    "@types/lodash.debounce": "4.0.3",
    "@types/node": "10.5.2",
    "@types/ramda": "0.25.35",
    "@types/react": "16.4.6",
    "@types/react-dom": "16.0.6",
    "@types/react-hot-loader": "4.1.0",
    "@types/react-i18next": "7.8.0",
    "@types/react-redux": "6.0.4",
    "@types/react-router-dom": "4.3.0",
    "@types/serve-static": "1.13.2",
    "@types/tapable": "1.0.4",
    "@types/webpack": "4.4.7",
    "@types/webpack-env": "1.13.6",
    "@types/xtend": "4.0.2",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.5",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "commander": "2.16.0",
    "css-loader": "1.0.0",
    "del": "3.0.0",
    "electron": "3.0.0-beta.2",
    "electron-builder": "20.24.4",
    "electron-debug": "2.0.0",
    "electron-devtools-installer": "2.2.4",
    "electron-packager": "12.1.0",
    "fork-ts-checker-webpack-plugin": "0.4.3",
    "gulp": "4.0.0",
    "gulp-changed": "3.2.0",
    "gulp-install": "1.1.0",
    "html-webpack-plugin": "3.2.0",
    "imagemin": "5.3.1",
    "imagemin-pngquant": "6.0.0",
    "imagemin-svgo": "6.0.0",
    "img-loader": "3.0.0",
    "jest": "23.4.1",
    "jest-styled-components": "5.0.1",
    "koa": "2.5.2",
    "koa-static": "5.0.0",
    "koa-webpack": "5.1.0",
    "less": "3.7.1",
    "less-loader": "4.1.0",
    "react-hot-loader": "4.3.3",
    "serve-static": "1.13.2",
    "spectron": "3.8.0",
    "style-loader": "0.21.0",
    "tree-kill": "1.2.0",
    "ts-import-plugin": "1.5.4",
    "ts-jest": "23.0.1",
    "ts-loader": "4.4.2",
    "ts-node": "7.0.0",
    "tslint": "5.11.0",
    "tslint-loader": "3.6.0",
    "typescript": "2.9.2",
    "url-loader": "1.0.1",
    "webpack": "4.16.1",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-dev-middleware": "3.1.3",
    "webpack-node-externals": "1.7.2",
    "xtend": "4.0.1"
  },
  "engines": {
    "node": ">=10.1.0",
    "npm": ">=6.0.1"
  },
  "jest": {
    "roots": [
      "app/src"
    ],
    "collectCoverageFrom": [
      "**/*.{ts,tsx}"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleDirectories": [
      "node_modules",
      "build/node_modules"
    ],
    "moduleNameMapper": {
      "^@assets/(.*)": "<rootDir>/app/src/renderer/__mocks__/file-mock.ts",
      "^@common/(.*)": "<rootDir>/app/src/common/$1",
      "^@logic/(.*)": "renderer/logic/$1",
      "^@components/(.*)": "renderer/components/$1"
    }
  },
  "build": {
    "appId": "com.electron.pinkcoin",
    "productName": "Pinkcoin Wallet UI",
    "directories": {
      "app": "build",
      "output": "dist"
    },
    "publish": {
      "provider": "github"
    }
  }
}
